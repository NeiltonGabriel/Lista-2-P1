/*
Dados os 3 lados de um triângulo, diga se ele é equilátero, isósceles ou escaleno
*/

#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

int main(){

    double lado1, lado2, lado3;

    scanf("%lf %lf %lf", &lado1, &lado2, &lado3);
    
    //escaleno?
    if (lado1 != lado2 && lado1 != lado3 && lado2 != lado3){
        printf("escaleno\n");
    }
    
    //equilatero?
    else if (lado1 == lado2 && lado1 == lado3){
        printf("equilatero\n");
    }
    
    //isoceles
    else{
        printf("isosceles\n");
    }
    return 0;
}
